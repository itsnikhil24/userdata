<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="stylesheets\stylesheetusers.css" />
  </head>
  <body>
      <div class="buttons" id="goback"><a href="/">Go Back</a></div> 
      <div class="container">
        <% users.forEach(function(profile){ %>
          <div class="card">
            <div id="userimage">
              <img
              src="data:image/<%=profile.img.contentType%>;base64,
                      <%=profile.img.data.toString('base64')%>"
            />
            </div>
            <h4><%= profile.names %></h4>
            <h4><%= profile.email %></h4>
            <div class="links">
             <div class="buttons"><a href="/edit/<%= profile._id %>">Edit User</a> </div> 
             <div class="buttons"><a href="/delete/<%= profile._id %>">delete User</a></div> 
              
            </div>
          </div>
       <% }) %>
      
    </div>
  </body>
</html>
